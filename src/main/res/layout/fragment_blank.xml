<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:tag="f2"
    android:layout_marginTop="?android:attr/actionBarSize"
    tools:context="com.snapgroup.Fragments.BlankFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.com.snapgroup.PaymentsActivity">
  <Button
            android:id="@+id/button14"
            android:layout_width="400dp"
            android:layout_height="14dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/paymentsLv"
            android:layout_marginTop="12dp"
            android:background="#000" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/button13"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:layout_marginTop="13dp"
            android:text="Name Of Group"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Payments"
            android:textColor="@color/orange"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_above="@+id/paymentsLv"
            android:layout_toRightOf="@+id/textView17"
            android:layout_toEndOf="@+id/textView17"
            android:layout_marginLeft="13dp"
            android:layout_marginStart="13dp" />

        <com.twotoasters.jazzylistview.JazzyListView
            android:id="@+id/paymentsLv"
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView17"
            android:layout_marginTop="19dp"
            android:fastScrollEnabled="true"
            android:divider="@null"
            android:dividerHeight="5dp"
            android:scrollingCache="true"
            app:effect="reverse_fly"
            app:only_animate_new_items="false"
            app:only_animate_fling="false"
            app:max_velocity="0" />



        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:text="Total"
            android:textColor="@color/orange"
            android:textSize="18sp"
            android:textStyle="bold"
            android:layout_below="@+id/button14"
            android:layout_alignLeft="@+id/textView17"
            android:layout_alignStart="@+id/textView17" />

        <TextView
            android:id="@+id/textView27"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="450$"
            android:textColor="@color/orange"
            android:textSize="18sp"
            android:textStyle="bold"
            android:layout_marginRight="11dp"
            android:layout_marginEnd="11dp"
            android:layout_above="@+id/relativeLayout"
            android:layout_toLeftOf="@+id/textView38"
            android:layout_toStartOf="@+id/textView38" />

        <RelativeLayout
            android:id="@+id/relativeLayout"
            android:layout_width="360dp"
            android:layout_height="200dp"
            android:layout_below="@+id/textView26"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            android:background="@drawable/linear_border">
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">
            <com.jjoe64.graphview.GraphView
                android:id="@+id/graph"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="8dp" />
</ScrollView>
        </RelativeLayout>
    </RelativeLayout>

</FrameLayout>
